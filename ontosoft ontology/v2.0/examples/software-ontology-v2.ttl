@prefix : <http://ontosoft.org/software#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix software: <http://ontosoft.org/software#> .
@base <http://ontosoft.org/software> .

<http://ontosoft.org/software> rdf:type owl:Ontology ;
                                owl:imports <http://ontosoft.org/softwareCategories> ;
                                terms:title "The Ontosoft ontology"@en ;
                                terms:description "Ontosoft is an ontology to describe software."@en ;
                                terms:modified "2018-04-21" ;
                                vann:preferredNamespaceUri "http://ontosoft.org/software#" ;
                                owl:versionInfo "2.0"^^xsd:string ;
                                vann:preferredNamespacePrefix "osw" ;
                                terms:created "2015-03-12"^^xsd:date ;
                                rdfs:comment "Ontosoft is an ontology to describe software"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://ontosoft.org/software#category
software:category rdf:type owl:AnnotationProperty ;
                  rdfs:comment "Category of software metadata properties"@en .


###  http://ontosoft.org/software#isRequired
software:isRequired rdf:type owl:AnnotationProperty ;
                    rdfs:comment "This property annotation indicates if a property is a required property or not"@en .


###  http://ontosoft.org/software#searchConfig
software:searchConfig rdf:type owl:AnnotationProperty ;
                      rdfs:comment "This property annotation provides information to be handed over to search engines (like Solr) to explain how to tokenize and index the property values"@en .


###  http://ontosoft.org/software#uiConfig
software:uiConfig rdf:type owl:AnnotationProperty ;
                  rdfs:comment "This property annotation provides UI specific information for the property like positioning, special UI interface, etc"@en .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#description
rdfs:description rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ontosoft.org/software#affectsSoftwarePart
software:affectsSoftwareFunction rdf:type owl:ObjectProperty ;
                             rdfs:domain software:KnownIssue ;
                             rdfs:range software:Function .


###  http://ontosoft.org/software#fixedKnownIssue
software:fixedKnownIssue rdf:type owl:ObjectProperty ;
                         rdfs:domain software:BugFix ;
                         rdfs:range software:KnownIssue .


###  http://ontosoft.org/software#hasActiveDevelopment
software:hasActiveDevelopment rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain software:Software ;
                              rdfs:range software:DevelopmentInformation ;
                              software:category <http://ontosoft.org/softwareCategories#Contribute> ;
                              software:uiConfig "{\"position\":1}" ;
                              dc:description "How is the software being developed or maintained?"@en .


###  http://ontosoft.org/software#hasAdopter
software:hasAdopter rdf:type owl:ObjectProperty ;
                    rdfs:domain software:Software ;
                    rdfs:range software:Adopter ;
                    software:category <http://ontosoft.org/softwareCategories#Trust> ;
                    software:uiConfig "{\"position\": 3}" ;
                    dc:description "Has the software been adopted in a project, organization or by a person?" .


###  http://ontosoft.org/software#hasAverageRunTime
software:hasAverageRunTime rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain software:SoftwareVersion ;
                           rdfs:range software:MeasurementEntity ;
                           software:category <http://ontosoft.org/softwareCategories#Install> ;
                           software:uiConfig "{\"position\":1}" ;
                           dc:description "Do you know how long it takes to run this software version on average?"@en .


###  http://ontosoft.org/software#hasBenchmarkInformation
software:hasBenchmarkInformation rdf:type owl:ObjectProperty ;
                                 rdfs:domain software:Software ;
                                 rdfs:range software:BenchmarkInformation ;
                                 software:category <http://ontosoft.org/softwareCategories#Trust> ;
                                 software:uiConfig "{\"position\":7}" ;
                                 dc:description "Is there any benchmark information about the software?"@en .


###  http://ontosoft.org/software#hasChange
software:hasBugFix rdf:type owl:ObjectProperty ;
                   rdfs:domain software:SoftwareVersion ;
                   rdfs:range software:BugFix ;
                   software:category <http://ontosoft.org/softwareCategories#Track> ;
                   dc:description "Are there any bug fixes that affects this software version?"@en .


###  http://ontosoft.org/software#hasChangeDescription
software:hasBugFixDescription rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf software:hasShortDescription ;
                              rdfs:domain software:BugFix .


###  http://ontosoft.org/software#hasCitationLocation
software:hasCitationLocation rdf:type owl:ObjectProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain software:Citation ;
                             rdfs:range software:Location ;
                             software:category <http://ontosoft.org/softwareCategories#Cite> ;
                             dc:description "What is the URL for the citation?"@en .


###  http://ontosoft.org/software#hasCitationText
software:hasCitationText rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain software:Citation ;
                         rdfs:range software:CitationText ;
                         software:category <http://ontosoft.org/softwareCategories#Cite> ;
                         dc:description "What is the text associated with the Citation?"@en .


###  http://ontosoft.org/software#hasCodeLocation
software:hasCodeLocation rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain software:SoftwareVersion ;
                         rdfs:range software:Location ;
                         software:category <http://ontosoft.org/softwareCategories#Access> ;
                         software:isRequired "true"^^xsd:boolean ;
                         software:uiConfig "{\"position\": 1}" ;
                         dc:description "What is the URL for the code?"@en .


###  http://ontosoft.org/software#hasCommitmentOfSupport
software:hasCommitmentOfSupport rdf:type owl:ObjectProperty ,
                                         owl:FunctionalProperty ;
                                rdfs:domain software:Software ;
                                rdfs:range software:TextEntity ;
                                software:category <http://ontosoft.org/softwareCategories#Trust> ;
                                software:uiConfig "{\"position\":2}" ;
                                dc:description "How can a user get support for the software? (eg. Report bugs, request features and extensions, etc)"@en .


###  http://ontosoft.org/software#hasCompatibleSoftware
software:hasCompatibleSoftware rdf:type owl:ObjectProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain software:Composition ;
                               rdfs:range software:SoftwareDescription ;
                               software:category <http://ontosoft.org/softwareCategories#Compose> ;
                               dc:description "What software can this software be composed with in a workflow?"@en .


###  http://ontosoft.org/software#hasCompositionDescription
software:hasCompositionDescription rdf:type owl:ObjectProperty ,
                                            owl:FunctionalProperty ;
                                   rdfs:domain software:Software ;
                                   rdfs:range software:Composition ;
                                   software:category <http://ontosoft.org/softwareCategories#Compose> ;
                                   software:uiConfig "{\"position\":1}" ;
                                   dc:description "Is this software typically used with other software in a workflow? (eg: for visualization, preprocessing, postprocessing, etc)"@en .


###  http://ontosoft.org/software#hasCreator
software:hasCreator rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf terms:creator ;
                    rdfs:domain software:Software ;
                    rdfs:range prov:Agent ;
                    software:category <http://ontosoft.org/softwareCategories#Trust> ;
                    software:isRequired "true"^^xsd:boolean ;
                    software:uiConfig "{\"position\":1}" ;
                    software:searchConfig "{\"facet\":\"Author\"}" ;
                    dc:description "Who created this software? (Project, Organization, Person, Initiative, etc.)"@en .





###  http://ontosoft.org/software#hasDependency
software:hasDependency rdf:type owl:ObjectProperty ;
                       rdfs:domain software:SoftwareVersion ;
                       rdfs:range software:SoftwareVersion ;
                       software:category <http://ontosoft.org/softwareCategories#Install> ;
                       software:isRequired "true"^^xsd:boolean ;
                       software:uiConfig "{\"position\":5}" ;
                       dc:description "What other software does the software version require to be installed?"@en .


###  http://ontosoft.org/software#hasDocumentation
software:hasDocumentation rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain software:SoftwareVersion ;
                          rdfs:range software:Location ;
                          software:category <http://ontosoft.org/softwareCategories#Install> ;
                          software:isRequired "true"^^xsd:boolean ;
                          software:uiConfig "{\"position\": 1}" ;
                          dc:description "Is there any on-line documentation about this software version?"@en .


###  http://ontosoft.org/software#hasDomainKeywords
software:hasDomainKeywords rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain software:SoftwareVersion ;
                           rdfs:range software:Keywords ;
                           software:category <http://ontosoft.org/softwareCategories#Relate> ;
                           software:isRequired "true"^^xsd:boolean ;
                           software:uiConfig "{\"position\":1}" ;
                           dc:description "What are the domain specific keywords for this software"@en .


###  http://ontosoft.org/software#hasEmailContact
software:hasEmailContact rdf:type owl:ObjectProperty ;
                         rdfs:domain software:Software ;
                         rdfs:range software:TextEntity ;
                         software:category <http://ontosoft.org/softwareCategories#Discuss> ;
                         software:uiConfig "{\"position\": 2}" ;
                         dc:description "What is the e-mail contact for this software?" .


###  http://ontosoft.org/software#hasExecutableLocation
software:hasExecutableLocation rdf:type owl:ObjectProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain software:SoftwareVersion ;
                               rdfs:range software:Location ;
                               software:category <http://ontosoft.org/softwareCategories#Access> ;
                               software:uiConfig "{\"position\": 1}" ;
                               dc:description "What is the URL for the executable?"@en .





###  http://ontosoft.org/software#hasFunction
software:hasFunction rdf:type owl:ObjectProperty ;
                     rdfs:domain software:SoftwareVersion ;
                     rdfs:range software:Function ;
                     software:category <http://ontosoft.org/softwareCategories#Compose> ;
                     software:isRequired "true"^^xsd:boolean ;
                     dc:description "What functions does this software version have?" .


###  http://ontosoft.org/software#hasFunctionName
software:hasFunctionName rdf:type owl:ObjectProperty , 
                                      owl:FunctionalProperty ;
                         rdfs:subPropertyOf software:hasName ;
                         software:isRequired "true"^^xsd:boolean ;
                         software:uiConfig "{\"position\": 1}" ;
                         rdfs:domain software:Function .

###  http://ontosoft.org/software#hasFunctionDescription
software:hasFunctionDescription rdf:type owl:ObjectProperty ,
                                        owl:FunctionalProperty ;
                                rdfs:subPropertyOf software:hasShortDescription ;
                                software:isRequired "true"^^xsd:boolean ;
                                software:uiConfig "{\"position\": 2}" ;
                                rdfs:domain software:Function .


###  http://ontosoft.org/software#hasFunctionality
software:hasFunctionality rdf:type owl:ObjectProperty ;
                          rdfs:domain software:Function ;
                          software:category <http://ontosoft.org/softwareCategories#Run> ;
                          software:searchConfig "{\"facet\":\"Functionality\"}" ;
                          software:uiConfig "{\"position\": 3}" ;
                          rdfs:range software:Functionality .

###  http://ontosoft.org/software#usesAlgorithm
software:usesAlgorithm rdf:type owl:ObjectProperty ;
                   rdfs:domain software:Function ;
                   rdfs:range software:Algorithm ;
                   software:category <http://ontosoft.org/softwareCategories#Run> ;
                   software:uiConfig "{\"position\": 4}" ;
                   software:searchConfig "{\"facet\":\"Algorithm\"}" ;
                   dc:description "What is the algorithm used in this function?"@en .

software:hasFunctionInvocation rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain software:Function ;
                     rdfs:range software:FunctionInvocation ;
                     software:category <http://ontosoft.org/softwareCategories#Run> ;
                     software:uiConfig "{\"position\":5}" ;
                     dc:description "What is the invocation line for this function?"@en .


###  http://ontosoft.org/software#hasInput
software:hasInputFile rdf:type owl:ObjectProperty ;
                  rdfs:domain software:Function ;
                  rdfs:range software:InputFile ;
                  software:category <http://ontosoft.org/softwareCategories#Run> ;
                  software:uiConfig "{\"position\":6}" ;
                  dc:description "What input files does this function require?"@en .


###  http://ontosoft.org/software#hasInputFileName
software:hasInputFileName rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                      rdfs:subPropertyOf software:hasName ;
                      software:uiConfig "{\"position\":1}" ;
                      dc:description "What is the input name?"@en ;
                      rdfs:domain software:InputFile .


###  http://ontosoft.org/software#hasInputFileDescription
software:hasInputFileDescription rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf software:hasShortDescription ;
                             software:uiConfig "{\"position\":2}" ;
                             dc:description "What is the input description?"@en ;
                             rdfs:domain software:InputFile .

###  http://ontosoft.org/software#hasInputFileArgument
software:hasInputFileArgument rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain software:InputFile ;
                         software:uiConfig "{\"position\":3}" ;
                         rdfs:range software:TextEntity ;
                         dc:description "What is the argument for this input file?" .

###  http://ontosoft.org/software#hasInputFileDataType
software:hasInputFileDataType rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain software:InputFile ;
                     rdfs:range software:DataType ;
                     software:uiConfig "{\"position\":4}" ;
                     software:category <http://ontosoft.org/softwareCategories#Run> ;
                     software:searchConfig "{\"facet\":\"Input File Data Type\"}" ;
                     dc:description "What is the input file type?"@en .


###  http://ontosoft.org/software#hasInputFileDataFormat
software:hasInputFileDataFormat rdf:type owl:ObjectProperty ;
                       rdfs:domain software:InputFile ;
                       software:uiConfig "{\"position\":5}" ;
                       software:searchConfig "{\"facet\":\"Input File Data Format\"}" ;
                       software:category <http://ontosoft.org/softwareCategories#Run> ;
                       dc:description "What is the data format for this input file?"@en ;
                       rdfs:range software:DataFormat .


###  http://ontosoft.org/software#hasInputFileDefaultValue
software:hasInputFileDefaultValue rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain software:InputFile ;
                         rdfs:range software:TextEntity ;
                         software:uiConfig "{\"position\":6}" ;
                         dc:description "What is the default value for this input?" .


###  http://ontosoft.org/software#hasInputParameter
software:hasInputParameter rdf:type owl:ObjectProperty ;
                           rdfs:domain software:Function ;
                           rdfs:range software:InputParameter ;
                           software:category <http://ontosoft.org/softwareCategories#Run> ;
                           software:uiConfig "{\"position\": 7}" ;
                           dc:description "What are the input parameters used for this function?" .

###  http://ontosoft.org/software#hasInputName
software:hasInputParameterName rdf:type owl:ObjectProperty  ,
                                  owl:FunctionalProperty ;
                      rdfs:subPropertyOf software:hasName ;
                      software:uiConfig "{\"position\":1}" ;
                      dc:description "What is this parameter name?"@en ;
                      rdfs:domain software:InputParameter .

###  http://ontosoft.org/software#hasInputDescription
software:hasInputParameterDescription rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf software:hasShortDescription ;
                             software:uiConfig "{\"position\":2}" ;
                             dc:description "What is the parameter description?"@en ;
                             rdfs:domain software:InputParameter .


###  http://ontosoft.org/software#hasInputParameterArgument
software:hasInputParameterArgument rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain software:InputParameter ;
                         rdfs:range software:TextEntity ;
                         software:uiConfig "{\"position\":3}" ;
                         dc:description "What is the argument for this input parameter?" .

###  http://ontosoft.org/software#hasInputParameterDataType
software:hasInputParameterDataType rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain software:InputParameter ;
                     rdfs:range software:DataType ;
                     software:uiConfig "{\"position\":4}" ;
                     software:searchConfig "{\"facet\":\"Input Parameter Data Type\"}" ;
                     software:category <http://ontosoft.org/softwareCategories#Run> ;
                     dc:description "What is the parameter type?"@en .


###  http://ontosoft.org/software#hasInputParameterDefaultValue
software:hasInputParameterDefaultValue rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain software:InputParameter ;
                         software:uiConfig "{\"position\":5}" ;
                         rdfs:range software:TextEntity ;
                         dc:description "What is the default value for this input parameter?" .

###  http://ontosoft.org/software#hasInputOrder
software:hasInputOrder rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf software:hasNumericValue ;
                       rdfs:domain software:InputFile .

###  http://ontosoft.org/software#hasOutput
software:hasOutput rdf:type owl:ObjectProperty ;
                   rdfs:domain software:Function ;
                   rdfs:range software:Output ;
                   software:category <http://ontosoft.org/softwareCategories#Run> ;
                   software:uiConfig "{\"position\":8}" ;
                   dc:description "What outputs does this function produce?"@en .

###  http://ontosoft.org/software#hasOutputName
software:hasOutputName rdf:type owl:ObjectProperty  ,
                                  owl:FunctionalProperty ;
                       rdfs:subPropertyOf software:hasName ;
                       software:uiConfig "{\"position\": 1}" ;
                       dc:description "What is the output name?" ;
                       rdfs:domain software:Output .


###  http://ontosoft.org/software#hasOutputDescription
software:hasOutputDescription rdf:type owl:ObjectProperty  ,
                                  owl:FunctionalProperty ;
                              rdfs:subPropertyOf software:hasShortDescription ;
                              software:uiConfig "{\"position\": 2}" ;
                              dc:description "What is the output description?" ;
                              rdfs:domain software:Output .


###  http://ontosoft.org/software#hasFileType
software:hasOutputDataType rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain software:Output ;
                     rdfs:range software:DataType ;
                     software:uiConfig "{\"position\": 3}" ;
                     software:searchConfig "{\"facet\":\"Output Data Type\"}" ;
                     software:category <http://ontosoft.org/softwareCategories#Compose> ;
                     dc:description "What is the File I/O type?"@en .


###  http://ontosoft.org/software#hasDataFormat
software:hasOutputDataFormat rdf:type owl:ObjectProperty ;
                       rdfs:domain software:Output ;
                       software:uiConfig "{\"position\":4}" ;
                       software:searchConfig "{\"facet\":\"Output Data Format\"}" ;
                       software:category <http://ontosoft.org/softwareCategories#Run> ;
                       dc:description "What is the data format for this output?" ;
                       rdfs:range software:DataFormat .

###  http://ontosoft.org/software#hasFundingSources
software:hasFundingSources rdf:type owl:ObjectProperty ;
                           rdfs:domain software:Software ;
                           rdfs:range software:TextEntity ;
                           software:category <http://ontosoft.org/softwareCategories#Trust> ;
                           software:uiConfig "{\"position\": 8}" ;
                           dc:description "What are the funding sources for this software?" .


###  http://ontosoft.org/software#hasImplementationLanguage
software:hasImplementationLanguage rdf:type owl:ObjectProperty ;
                                   rdfs:domain software:SoftwareVersion ;
                                   rdfs:range software:ProgrammingLanguage ;
                                   software:category <http://ontosoft.org/softwareCategories#Install> ;
                                   software:isRequired "true"^^xsd:boolean ;
                                   software:uiConfig "{\"position\":2}" ;
                                   software:searchConfig "{\"facet\":\"Programming Language\"}" ;
                                   dc:description "What language(s) is this software version written in?"@en .


###  http://ontosoft.org/software#hasInstallationInstructions
software:hasInstallationInstructions rdf:type owl:ObjectProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain software:SoftwareVersion ;
                                     rdfs:range software:TextEntity ;
                                     software:category <http://ontosoft.org/softwareCategories#Install> ;
                                     software:isRequired "true"^^xsd:boolean ;
                                     software:uiConfig "{\"position\":4}" ;
                                     dc:description "How can one install this software version?"@en .


###  http://ontosoft.org/software#hasInteroperableSoftware
software:hasInteroperableSoftware rdf:type owl:ObjectProperty ;
                                  rdfs:domain software:Software ;
                                  rdfs:range software:SoftwareDescription ;
                                  software:category <http://ontosoft.org/softwareCategories#Compose> ;
                                  software:isRequired "true"^^xsd:boolean ;
                                  software:uiConfig "{\"position\": 1}" ;
                                  dc:description "What other software can interoperate with this one?" .


###  http://ontosoft.org/software#hasKnownIssue
software:hasKnownIssue rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain software:SoftwareVersion ;
                       software:category <http://ontosoft.org/softwareCategories#Trust> ;
                       dc:description "What known issues does this software version have?" ;
                       rdfs:range software:KnownIssue .


###  http://ontosoft.org/software#hasKnownIssueDescription
software:hasKnownIssueDescription rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                                  rdfs:subPropertyOf software:hasShortDescription ;
                                  dc:description "What description does this known issues have?" ;
                                  rdfs:domain software:KnownIssue .


###  http://ontosoft.org/software#hasLatestSoftwareVersion
software:hasLatestSoftwareVersion rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                                  rdfs:subPropertyOf software:hasSoftwareVersion ;
                                  rdfs:domain software:Software ;
                                  software:category <http://ontosoft.org/softwareCategories#Track> ;
                                  dc:description "What is the latest software version for this software?" ;
                                  rdfs:range software:SoftwareVersion .


###  http://ontosoft.org/software#hasLicense
software:hasLicense rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf terms:license ;
                    rdfs:domain software:SoftwareVersion ;
                    rdfs:range software:License ;
                    software:category <http://ontosoft.org/softwareCategories#Access> ;
                    software:isRequired "true"^^xsd:boolean ;
                    software:uiConfig "{\"position\": 3}" ;
                    software:searchConfig "{\"facet\":\"License\"}" ;
                    dc:description "What license is the code released under?"@en .


###  http://ontosoft.org/software#hasMajorContributor
software:hasMajorContributor rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf terms:contributor ;
                             rdfs:domain software:SoftwareVersion ;
                             rdfs:range prov:Agent ;
                             software:category <http://ontosoft.org/softwareCategories#Trust> ;
                             software:isRequired "true"^^xsd:boolean ;
                             software:uiConfig "{\"position\":2}" ;
                             dc:description "Are there any major contributors of note for this software version?"@en .


###  http://ontosoft.org/software#hasName
software:hasName rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:range software:TextEntity ;
                 software:category <http://ontosoft.org/softwareCategories#Locate> ;
                 software:isRequired "true"^^xsd:boolean ;
                 software:uiConfig "{\"position\": 1}" ;
                 dc:description "What is the software called?"@en .


###  http://ontosoft.org/software#hasOtherImplementationDetails
software:hasOtherImplementationDetails rdf:type owl:ObjectProperty ,
                                                owl:FunctionalProperty ;
                                       rdfs:domain software:SoftwareVersion ;
                                       rdfs:range software:ImplementationDetails ;
                                       software:category <http://ontosoft.org/softwareCategories#Install> ;
                                       software:uiConfig "{\"position\":3}" ;
                                       dc:description "Are there any other important details about the implementation of this code (parallelization, special hardware, etc)?"@en .


###  http://ontosoft.org/software#hasPersonIdentifier
software:hasPersonIdentifier rdf:type owl:ObjectProperty ;
                             rdfs:domain software:Person ;
                             rdfs:range software:Identifier ;
                             software:category <http://ontosoft.org/softwareCategories#Identify> ;
                             software:isRequired "true" ;
                             dc:description "Provide a Twitter handle, ORCID Id, email address, github URL, home page, or any other unique identifier for this person"@en .


###  http://ontosoft.org/software#hasPreferredCitation
software:hasPreferredSoftwareCitation rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain software:Software ;
                              rdfs:range software:Citation ;
                              software:category <http://ontosoft.org/softwareCategories#Cite> ;
                              software:isRequired "true"^^xsd:boolean ;
                              software:uiConfig "{\"position\":1}" ;
                              dc:description "Is there a preferred publication or citation for this software?"@en .

###  http://ontosoft.org/software#hasPreferredCitation
software:hasPreferredSoftwareVersionCitation rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain software:SoftwareVersion ;
                              rdfs:range software:Citation ;
                              software:category <http://ontosoft.org/softwareCategories#Cite> ;
                              software:isRequired "true"^^xsd:boolean ;
                              software:uiConfig "{\"position\":1}" ;
                              dc:description "Is there a preferred publication or citation for this software version?"@en .


###  http://ontosoft.org/software#hasProjectWebsite
software:hasProjectWebsite rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain software:Software ;
                           rdfs:range software:Location ;
                           software:category <http://ontosoft.org/softwareCategories#Locate> ;
                           software:isRequired "true"^^xsd:boolean ;
                           software:uiConfig "{\"position\": 4}" ;
                           dc:description "Is there a project website for the software?"@en .


###  http://ontosoft.org/software#hasPublisher
software:hasPublisher rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain software:Software ;
                      rdfs:range software:Publisher ;
                      software:category <http://ontosoft.org/softwareCategories#Trust> ;
                      software:uiConfig "{\"position\": 1}" ;
                      dc:description "Who is the publisher of this software?" .


###  http://ontosoft.org/software#hasRating
software:hasRating rdf:type owl:ObjectProperty ,
